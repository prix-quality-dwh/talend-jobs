    <elementParameter field="TEXT" name="USER" show="false" value="&quot;&quot;"/>
    <elementParameter field="PASSWORD" name="PASS" show="false" value="enc:system.encryption.key.v1:lzlzKGa9jDiSa3TntL5ZD27kVeXxYu9y3Zouifk/"/>
    <elementParameter field="DBTABLE" name="TABLE" value="&quot;work_timbrature_fase0 =&gt; work_timbrature_fase1 (tempo effettivo a 30 minuti)&quot;"/>
    <elementParameter field="QUERYSTORE_TYPE" name="QUERYSTORE" value="&quot;&quot;"/>
    <elementParameter field="TECHNICAL" name="QUERYSTORE:REPOSITORY_QUERYSTORE_TYPE" show="false" value=""/>
    <elementParameter field="TECHNICAL" name="QUERYSTORE:QUERYSTORE_TYPE" value="BUILT_IN"/>
    <elementParameter field="MEMO_SQL" name="QUERY" value="&quot;--calcolo minuti e ore a 30 minuti &#13;&#10;INSERT INTO DWH_INT.work_timbrature_fase1 (cod_fisc_ris, codice_dipendente, data, orario, verso, tipotimbratura, codter, descrter, data_prec, orario_prec, verso_prec, verso_inizio, data_ora, minuti, ora, minuti_effettivi, ora_effettiva, data_ora_prec, minuti_prec, ora_prec, minuti_prec_effettivi, ora_prec_effettiva )&#13;&#10;SELECT &#13;&#10;  cod_fisc_ris&#13;&#10;, codice_dipendente&#13;&#10;, data&#13;&#10;, orario&#13;&#10;, verso&#13;&#10;, tipotimbratura&#13;&#10;, codter&#13;&#10;, descrter&#13;&#10;, data_prec&#13;&#10;, orario_prec&#13;&#10;, verso_prec&#13;&#10;, verso_inizio&#13;&#10;---calcolo tempi sulla data &#13;&#10;, case when verso = 'E' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 31 and 59  &#13;&#10;then TIMESTAMPADD('minute',(60-MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP)), TIMESTAMPADD('minute', orario, data)::TIMESTAMP)::TIMESTAMP &#13;&#10;else TIMESTAMPADD('minute', orario, data)::TIMESTAMP end as data_ora --gestione data che potrebbe cambiare a cavallo di mezzanote seguito al calcolo tempo a mezz'ora&#13;&#10;, MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) as minuti&#13;&#10;, HOUR(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) as ora&#13;&#10;---calcolo minuti E/U a 30 minuti (Entrata --&gt; in eccesso, Uscita --&gt; in difetto) &#13;&#10;,case when verso = 'E' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 1 and 29 then 30&#13;&#10; when verso = 'E' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 31 and 59 then 0 &#13;&#10; when verso = 'U' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 1 and 29 then 0&#13;&#10; when verso = 'U' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 31 and 59 then 30&#13;&#10; else MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) end as minuti_effettivi&#13;&#10;---calcolo ore E/U a 30 minuti (Entrata --&gt; in eccesso, Uscita --&gt; in difetto) &#13;&#10;,case when verso = 'E' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 1 and 29 then HOUR(TIMESTAMPADD('minute', orario, data)::TIMESTAMP)&#13;&#10; when verso = 'E' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 31 and 59 then HOUR(TIMESTAMPADD('minute',(60-MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP)), TIMESTAMPADD('minute', orario, data)::TIMESTAMP)::TIMESTAMP)&#13;&#10; when verso = 'U' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 1 and 29 then HOUR(TIMESTAMPADD('minute', orario, data)::TIMESTAMP)&#13;&#10; when verso = 'U' and MINUTE(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) BETWEEN 31 and 59 then HOUR(TIMESTAMPADD('minute', orario, data)::TIMESTAMP)&#13;&#10; else HOUR(TIMESTAMPADD('minute', orario, data)::TIMESTAMP) end as ora_effettiva&#13;&#10; ---calcolo tempi sulla data precedente&#13;&#10;, case when verso = 'E' and MINUTE(TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP) BETWEEN 31 and 59  &#13;&#10;then TIMESTAMPADD('minute',(60-MINUTE(TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP)), TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP)::TIMESTAMP &#13;&#10;else TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP end as data_ora_prec --gestione data che potrebbe cambiare a cavallo di mezzanote seguito al calcolo tempo a mezz'ora&#13;&#10;, MINUTE(TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP) as minuti_prec&#13;&#10;, HOUR(TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP) as ora_prec&#13;&#10;---calcolo minuti E/U a 30 minuti (Entrata --&gt; in eccesso, Uscita --&gt; in difetto) &#13;&#10;,case when verso_prec = 'E' and MINUTE(TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP) BETWEEN 1 and 29 then 30&#13;&#10; when verso_prec = 'E' and MINUTE(TIMESTAMPADD('minute', orario_prec, data_prec)::TIMESTAMP) BETWEEN 31 and 59 then 